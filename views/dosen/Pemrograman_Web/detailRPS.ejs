<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8">
    <!--<title> Responsiive Admin Dashboard | CodingLab </title>-->
    <link rel="stylesheet" href="/assets/css/main.css">
    <link rel="stylesheet" href="/assets/css/style.css">
    <!-- Boxicons CDN Link -->
    <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />
  </head>

  <body>
    <div class="sidebar">
      <div class="logo-details">
        <img src = "https://upload.wikimedia.org/wikipedia/commons/thumb/0/01/Logo_Unand.svg/1200px-Logo_Unand.svg.png" 
          alt="Unand" width="35" height="35" style="margin:0px 12.5px">
        <span class="logo_name">Manajemen RPS</span>
      </div>
      
      <div class="nav-links">
        <li>
          <a href="dashboardDosen">
            <i class='bx bx-grid-alt' ></i>
            <span class="links_name">Dashboard</span>
          </a>
        </li>
        <li>
          <a href="listRPS">
            <i class='bx bx-list-ul' ></i>
            <span class="links_name">List RPS</span>
          </a>
        </li>
        <li>
          <a href="detailRPS?id=1">
            <i class='bx bx-box' ></i>
            <span class="links_name">Detail RPS</span>
          </a>
        </li>
        <li class="log_out">
          <a href="../../login" onclick="return confirm('Apakah Anda yakin ingin Log out?');">
            <i class='bx bx-log-out'></i>
            <span class="links_name">Log out</span>
          </a>
        </li>
      </div>
    </div>

    <section class="home-section">
      <nav>
        <div class="sidebar-button">
          <i class='bx bx-menu sidebarBtn'></i>
          <span class="dashboard">Pemrograman Web (JSI62165)</span>
        </div>
        <div class="profile-details">
          <!--<img src="images/profile.jpg" alt="">-->
          <span class="admin_name">Husnil Kamil</span>
          <i class='bx bx-chevron-down' ></i>
        </div>
      </nav>

      <div class="home-content">
        <div style="margin:0px 20px ;">
          <h5>Nama Mata Kuliah: <%= course.name %></h5>
          <h5>Kode Mata Kuliah: <%= course.code %></h5>
          <h5>Dosen Pengampu: <% dosen.forEach((d,i)=>{ %>
            <%= d.lecturer.name %>,
          <%  }) %></h5>
          <h5>Semester: <%=course.semester%></h5>
          <h5>Bobot SKS: <%=course.credit%></h5>
        </div>
        &nbsp;

        <div style="margin:0px 20px ;">
          <h5>Deskripsi Mata Kuliah</h5>
          <p>
            <% if (course.description){ %>
              <%=course.description%>.
            <% }else { %>
              Deskripsi Belum ada
            <% } %>
          </p>
        </div>
        &nbsp;

        <div style="margin:0px 20px ;">
          <h5>Peta CPL ke CPMK</h5>
          <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">CP Program Studi</th>
                <th scope="col">Keterangan</th>
              </tr>
            </thead>
            <tbody>
              <% cpl.forEach((c,i)=>{%>
              <tr>
                <th scope="row"><%= c.code %></th>
                <td><%= c.name %></td>
              </tr>
              <% }) %>
            </tbody>
          </table>

          <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">CP Mata Kuliah</th>
                <th scope="col">Keterangan</th>
              </tr>
            </thead>
            <tbody>
              <% cpmk.forEach((cp,a)=>{ %>
              <tr>
                <th scope="row"><%=a+1%></th>
                <td><%= cp.name %>. 
                  <!-- (
                  <% cpl.forEach((c,i)=>{%>
                    <%= cp.course_lo_details.curriculum_lo_id %>
                  <% }) %>
                ) -->
              </td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
        &nbsp;

        <div style="margin:0px 20px ;">
          <h5>Referensi Utama</h5>
          <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">No</th>
                <th scope="col">Referensi</th>
              </tr>
            </thead>
            <tbody>
              <% ref.forEach((r,i)=>{%>
              <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=r.author%>, <%=r.year%>, “<%=r.title%>”, <%=r.publisher%></td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
        &nbsp;

        <div style="margin:0px 20px ;">
          <h5>Komponen Penilaian</h5>
          <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">No</th>
                <th scope="col">Penilaian</th>
                <th scope="col">Bobot</th>
              </tr>
            </thead>
            <tbody>
              <% asses.forEach((a,i)=>{%>
              <tr>
                <th scope="row"><%=i+1%></th>
                <td><%=a.name%></td>
                <td><%=a.percentage%>%</td>
              </tr>
              <%})%>
            </tbody>
          </table>
        </div>
        &nbsp;

        <div style="margin:0px 20px ;">
          <h5>Pertemuan Mingguan</h5>
          <table class="table table-bordered">
            <thead>
              
              <tr>
                <th scope="col">Minggu ke-</th>
                <th scope="col">Kemampuan akhir yang diharapkan</th>
                <th scope="col">Bahan Kajian (Materi Ajar) dan Referensi</th>
                <th scope="col">Metode Pembelajaran dan Alokasi Waktu</th>
                <th scope="col">Pengalaman Belajar Mahasiswa</th>
               
              </tr>
            </thead>
            <tbody>
              <% weekMeet.forEach((w,i)=>{ %>
              <tr>
                <th scope="row"><%=i+1%></th>
                <td>
                  <%=cpmk[i].name%>
                </td>
                <td><%=w.material%></td>
                <td><%=w.method%></td>
                <td><%=w.student_experience%></td>
                
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </section>
    
    <script>
      let sidebar = document.querySelector(".sidebar");
      let sidebarBtn = document.querySelector(".sidebarBtn");
      sidebarBtn.onclick = function() {
        sidebar.classList.toggle("active");
        if(sidebar.classList.contains("active")) {
          sidebarBtn.classList.replace("bx-menu" ,"bx-menu-alt-right");
        }
        else {
          sidebarBtn.classList.replace("bx-menu-alt-right", "bx-menu");
        }
      }
    </script>

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <!-- Modal -->
    <div class="modal fade" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
      <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-body">
                <h5>Hapus data ini?</h5>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Batal</button>
                <button type="button" href="#" class="btn btn-danger">Hapus</button>
            </div>
          </div>
      </div>
    </div>
  </body>
</html>